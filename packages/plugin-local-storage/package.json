{
	"name": "@protoheadless/plugin-local-storage",
	"version": "0.1.0",
	"description": "The official Local Storage plugin for Proto Headless",
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "tsup --watch",
		"format": "biome format --write ./src",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --apply ./src"
	},
	"files": [
		"dist/**"
	],
	"author": "Proto Digital",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ProtoDigitalUK/proto_headless.git",
		"directory": "packages/plugin-local-storage"
	},
	"keywords": [
		"CMS",
		"Headless CMS",
		"Proto Headless",
		"Proto Digital",
		"Local Storage",
		"Plugin"
	],
	"devDependencies": {
		"@biomejs/biome": "^1.6.4",
		"@protoheadless/config": "*",
		"@protoheadless/headless": "*",
		"@types/node": "^20.2.1",
		"tsup": "^8.0.2",
		"typescript": "^5.4.4"
	},
	"peerDependencies": {
		"@protoheadless/headless": "^0.0.1"
	},
	"dependencies": {
		"fs-extra": "^11.2.0",
		"mime-types": "^2.1.35"
	},
	"volta": {
		"node": "21.6.2"
	},
	"license": "MIT"
}
