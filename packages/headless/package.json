{
	"name": "@protodigital/headless",
	"version": "0.0.1",
	"private": true,
	"description": "The core of the Headless CMS. Its responsible for spinning up the API and serving the CMS.",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "tsup --watch",
		"generate": "npx kysely-codegen --out-file ../../node_modules/kysely-codegen/dist/db.d.ts",
		"format": "npx @biomejs/biome format --write ./src",
		"clear-db": "node ./clear-db.js"
	},
	"files": [
		"dist/**",
		"cms/**",
		"templates/**"
	],
	"author": "William Yallop",
	"devDependencies": {
		"@headless/types": "*",
		"@types/fs-extra": "^11.0.1",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/lodash.merge": "^4.6.9",
		"@types/node": "^20.2.1",
		"@types/node-cron": "^3.0.11",
		"@types/pg": "^8.10.1",
		"@types/sanitize-html": "^2.9.0",
		"@types/slug": "^5.0.7",
		"install": "^0.13.0",
		"kysely-codegen": "^0.11.0",
		"npm": "^10.4.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	},
	"volta": {
		"node": "21.6.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.515.0",
		"@fastify/cookie": "^9.3.1",
		"@fastify/cors": "^9.0.1",
		"@fastify/multipart": "^8.1.0",
		"@fastify/static": "^7.0.1",
		"@fastify/swagger": "^8.14.0",
		"@fastify/swagger-ui": "^2.1.0",
		"argon2": "^0.31.2",
		"console-log-colors": "^0.4.0",
		"date-fns": "^3.3.1",
		"dotenv": "^16.4.1",
		"fastify": "^4.26.0",
		"fastify-plugin": "^4.5.1",
		"fs-extra": "^11.2.0",
		"handlebars": "^4.7.8",
		"iso-639-1": "^3.1.0",
		"jsonwebtoken": "^9.0.2",
		"kysely": "^0.27.2",
		"lodash.merge": "^4.6.2",
		"mime-types": "^2.1.35",
		"mjml": "^4.15.3",
		"node-cron": "^3.0.3",
		"pg": "^8.11.3",
		"sanitize-html": "^2.11.0",
		"sharp": "^0.33.2",
		"slug": "^8.2.3",
		"unique-names-generator": "^4.7.1",
		"zod": "^3.22.4"
	},
	"license": "UNLICENSED"
}
