{
	"name": "@protoheadless/plugin-s3",
	"version": "1.0.0",
	"private": false,
	"description": "The official S3 plugin for Proto Headless",
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "tsup --watch",
		"format": "biome format ./src",
		"format:fix": "biome format --write ./src",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --apply ./src"
	},
	"files": [
		"dist"
	],
	"author": "Proto Digital",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ProtoDigitalUK/proto_headless.git",
		"directory": "packages/plugin-s3"
	},
	"keywords": [
		"CMS",
		"Headless CMS",
		"Proto Headless",
		"Proto Digital",
		"S3",
		"Plugin"
	],
	"devDependencies": {
		"@biomejs/biome": "^1.6.4",
		"@protoheadless/config": "*",
		"@protoheadless/core": "*",
		"@types/node": "^20.2.1",
		"@smithy/types": "^2.12.0",
		"tsup": "^8.0.2",
		"typescript": "^5.4.4"
	},
	"peerDependencies": {
		"@protoheadless/core": "^0.1.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.550.0"
	},
	"volta": {
		"node": "21.6.2"
	},
	"license": "MIT"
}
