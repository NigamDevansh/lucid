{
	"name": "@protoheadless/plugin-nested-documents",
	"private": true,
	"description": "The official Nested Documents plugin for Proto Digital Headless",
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "tsup --watch",
		"format": "biome format ./src",
		"format:fix": "biome format --write ./src",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --apply ./src"
	},
	"files": ["dist/**"],
	"author": "Proto Digital",
	"devDependencies": {
		"@protoheadless/config": "*",
		"@protoheadless/core": "*",
		"@types/node": "^20.2.1",
		"@biomejs/biome": "^1.6.4",
		"tsup": "^8.0.2",
		"typescript": "^5.4.4"
	},
	"peerDependencies": {
		"@protoheadless/core": "^0.1.0"
	},
	"dependencies": {},
	"volta": {
		"node": "21.6.2"
	},
	"license": "MIT"
}
