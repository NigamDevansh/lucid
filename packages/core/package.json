{
  "name": "@lucid-cms/core",
  "version": "0.0.0",
  "private": false,
  "description": "The core of the Lucid CMS. Its responsible for spinning up the API and serving the CMS.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsup",
    "dev": "tsup --watch",
    "lint": "eslint --fix ./src/*.ts",
    "test": "jest --silent --coverage --detectOpenHandles",
    "clear-db": "node ./clear-db.js"
  },
  "files": [
    "dist/**",
    "cms/**",
    "templates/**"
  ],
  "author": "William Yallop",
  "devDependencies": {
    "@lucid/types": "*",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/fs-extra": "^11.0.1",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/mjml": "^4.7.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.1",
    "@types/nodemailer": "^6.4.8",
    "@types/pg": "^8.10.1",
    "@types/sanitize-html": "^2.9.0",
    "@types/sharp": "^0.32.0",
    "@types/slug": "^5.0.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.363.0",
    "argon2": "^0.30.3",
    "console-log-colors": "^0.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.7",
    "iso-639-1": "^3.1.0",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "mjml": "^4.14.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "pg": "^8.11.3",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.32.1",
    "slug": "^8.2.3",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "volta": {
    "node": "20.1.0"
  }
