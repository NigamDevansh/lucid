{
    "extends": "astro/tsconfigs/strictest",
    "compilerOptions": {
        "resolveJsonModule": true,
        "module": "ESNext",
        "target": "ESNext",
        "esModuleInterop": true,
        "moduleResolution": "Node",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "skipLibCheck": true,
    },
}